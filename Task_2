n1=float(input("Enter number 1: "))
n2=float(input("Enter number 2: "))
print("Operations:")
print("1.Addition")
print("2.Subtraction")
print("3.Multiplication")
print("4.Division")
choice=input("Enter a number to perform an operation: ")
if choice=='1':
    print("Result:",n1+n2)
elif choice=='2':
    print("Result:",n1-n2)
elif choice=='3':
    print("Result:",n1*n2)
elif choice=='4':
    print("Result:",n1/n2)
else:
    print("Enter a valid choice")    
